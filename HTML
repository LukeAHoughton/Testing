<!----- add class to bogy tag when section is visible  <script>
 const observer=new IntersectionObserver((e=>
{for(const t of e)if(t.isIntersecting){const e=t.target.getAttribute("data-section-theme");document.querySelector("body").classList.remove("white","white-bold","bright-inverse","dark-bold","light","light-bold","bright","dark","black","black-bold"),document.querySelector("body").classList.add(e)}})),sections=document.querySelectorAll(".page-section");for(const e of sections)observer.observe(e); </script>

----------->

<!---------
<script>

  const sectionColors = ["white","white-bold","bright-inverse","dark-bold","light","light-bold","bright","dark","black","black-bold"];

$(window).scroll(function() {
    // Get all sections
    const sections = $(":not(#footer-sections) >
 .page-section");
    let currentSection = null;
    // Iterate over each section
    sections.each(function() {
        // Check if the section is at the top of the viewport
        if (!($(this).position().top <= $(window).scrollTop())) {
            currentSection = $(this);
        }
    });

    if(currentSection !== null){
        // Get the value of the data-section-theme attribute
        const theme = currentSection.data("section-theme");
        // Remove the previous class from the body
        $("body").removeClass("white white-bold bright-inverse dark-bold light light-bold bright dark black black-bold");
        // Add the new class to the body
        $("body").addClass(theme);
    }
});
</script>

