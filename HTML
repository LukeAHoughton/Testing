<script>
/*document.addEventListener("DOMContentLoaded",(function(){document.querySelector(".sqs-search-ui-button-wrapper").addEventListener("click",(function(e){e.preventDefault(),this.classList.add("search-input-selected")})),document.querySelector(".search-input").addEventListener("focusout",(function(){document.querySelector(".sqs-search-ui-button-wrapper").classList.remove("search-input-selected")}))}));*/
/*
document.addEventListener("DOMContentLoaded", function() {
    document.querySelector(".sqs-search-ui-button-wrapper").addEventListener("click", function(e) {
        e.preventDefault();
        this.classList.add("search-input-selected");
        checkForResults();
    });
    document.querySelector(".search-input").addEventListener("input", function() {
        document.querySelector(".sqs-search-ui-button-wrapper").classList.remove("search-input-selected");
        checkForResults();
    });
});

function checkForResults() {
    var numResults = document.querySelectorAll(".search-result div").length;
    if (numResults >
 0) {
        document.querySelector(".sqs-search-ui-button-wrapper").classList.add("search-input-results");
    } else {
        document.querySelector(".sqs-search-ui-button-wrapper").classList.remove("search-input-results");
    }
}
*/
  
  const searchResultNotice = document.querySelector('.search-result-notice');
const searchResults = document.querySelector('div.search-results');

const observer = new MutationObserver(mutationsList =>
 {
  for(let mutation of mutationsList) {
    if (mutation.type === 'attributes' && mutation.attributeName === 'class') {
      if(searchResultNotice.classList.contains('hide')) {
        document.body.classList.add('search-results');
      } else {
        document.body.classList.remove('search-results');
      }
    }
  }
});

observer.observe(searchResultNotice, { attributes: true });

</script>


<script>

$(document).ready(function(){
$(".sqs-search-ui-button-wrapper").click(function(e){
e.preventDefault();
$(this).addClass("search-input-class");
});
$(document).on('focusout', '.search-input', function(){
$(".sqs-search-ui-button-wrapper").removeClass("search-input-class");
});
</script>


