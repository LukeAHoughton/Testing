$(document).ready((function() {$(".header-nav-list").each((function() {$(this).find(".header-nav-item--folder").each((function() {$(this).find(".header-nav-folder-content").each((function() {$(this).find('.desktop-header-menu-accordion').each((function() {$(this).append('<span class="header-nav-folder-item-displayer">
</span>
') })) })) })) })); }));

$(document).ready( function() {  $(".header-nav-item").each(function(){ $(this).find('.desktop-header-menu-accordion').each(function( i ){ $(this).addClass('header-menu-accordion-'+ (i+1)); }); }); $(".desktop-header-menu-accordion").each(function( i ){ $(this).addClass('header-menu-accordion'); }); });

const sqsrteClass="sqsrte-small",hmaClass="header-menu-accordion",hcaClass="header-child-accordion-next-level",setNClass=s=>
hcaClass+"-set-"+s;$(document).ready(function(){$(".header-nav-folder-content").each(function(){$(this).find("."+hmaClass).each(function(){var s=this.className.match(/header-menu-accordion-(\d+)/)[1];$(this).nextUntil("."+hmaClass).addClass(sqsrteClass).addClass(hcaClass).addClass(setNClass(s)).wrapAll('<div class="header-child-accordion" />
')})})});

$(document).ready( function() {$(".header-nav-folder-title[href*='/accordion'] ~ .header-nav-folder-content").each(function(){$(this).find('.header-child-accordion').each(function(i){$(this).addClass('header-child-accordion-set-'+ (i+1)); }); }); }); 

  $(document).ready( function() {$('.header-nav-list .header-nav-item:nth-child(1) .header-nav-folder-content').prepend('<p style="text-align:center;white-space:pre-wrap;" class="menu-declaration sqsrte-small">
Knowledge, skills and experience put into practice. <a href="/praxis">
