<script>

  
function addDesktopHeaderMenuAccordionClass(){$(".header-nav-list .header-nav-item--folder .header-nav-folder-title[href*='accordion'] ~ .header-nav-folder-content .header-nav-folder-item a:contains('Accordion')").closest(".header-nav-folder-item").addClass("desktop-header-menu-accordion")}function removeAccordionFromLink(){const e=/Accordion /gi;$(".header-nav-folder-item a:contains('Accordion')").contents().each((function(){3===this.nodeType&&e.test(this.nodeValue)&&(this.nodeValue=this.nodeValue.replace(e,""))}))}function addDisplayerToAccordion(){$(".header-nav-list .header-nav-item--folder .header-nav-folder-content .desktop-header-menu-accordion").append('<span class="header-nav-folder-item-displayer">
</span>
')}function addAccordionClass(){$(".header-nav-item").each((function(){var e=1;$(this).find(".desktop-header-menu-accordion").each((function(){$(this).addClass("header-menu-accordion-"+e),e++}))})),$(".desktop-header-menu-accordion").addClass("header-menu-accordion")}$(document).ready((function(){addDesktopHeaderMenuAccordionClass(),removeAccordionFromLink()})),$(document).ready((function(){addDisplayerToAccordion(),addAccordionClass()}));const sqsrteClass="sqsrte-small",hmaClass="header-menu-accordion",hcaClass="header-child-accordion-next-level",setNClass=e=>
hcaClass+"-set-"+e;function addClassesToAccordionItems(){$(".header-nav-folder-content").each((function(){$(this).find("."+hmaClass).each((function(){var e=this.className.match(/header-menu-accordion-(\d+)/)[1];$(this).nextUntil("."+hmaClass).addClass(sqsrteClass).addClass(hcaClass).addClass(setNClass(e)).wrapAll('<div class="header-child-accordion" />
')}))}))}function addAccordionSetClass(){$(".header-nav-item").each((function(){$(this).find(".header-child-accordion").each((function(e){$(this).addClass("header-child-accordion-set-"+(e+1))}))}))}$(document).ready((function(){addClassesToAccordionItems()})),$(document).ready((function(){addAccordionSetClass()}));var menuDeclarations=[{text:"Knowledge, skills and experience put into practice.",href:"/praxis"},{text:"A simple method of problem-solving.",href:"/online"},{text:"Expanding business assets.",href:"/projektx"},{text:"Testing & validating business concepts.",href:"/projektbeta"},{text:"Website Identity Packages & Professional Subscriptions.",href:"/projektsite"},{text:"Intensifying presence and experience.",href:"/projektprint"},{text:"Training practical & curious self-development.",href:"/projektintel"},{text:"Connect and engage with us.",href:"/potential"}];function prependMenuDeclarations(){$(".header-nav-list .header-nav-item .header-nav-folder-content").each((function(e){var n=menuDeclarations[e];if(n){var a='<p style="text-align:center;white-space:pre-wrap;" class="menu-declaration sqsrte-small">
'+n.text+' <a href="'+n.href+'">
Learn more</a>
</p>
';$(this).prepend(a)}}))}function desktopMenuAccordion(){$(".desktop-header-menu-accordion").each((function(){$(this).find(".header-nav-folder-item-displayer").click((function(e){e.preventDefault();var n=$(this).closest(".desktop-header-menu-accordion").hasClass("arrow-up");$(this).closest(".desktop-header-menu-accordion").parent(".header-nav-folder-content").find(">
 *").removeClass("arrow-up"),n||$(this).closest(".desktop-header-menu-accordion").addClass("arrow-up")}))}))}function removeArrowOnHover(){$(".header-nav-item").hover((function(e){$(this).find(".desktop-header-menu-accordion").each((function(){e.preventDefault(),$(this).hasClass("arrow-up")&&$(this).removeClass("arrow-up")}))}))}function numerateNoSecondLevelMenuItems(){$(".header-nav-item").each((function(){$(this).find(".header-nav-folder-content").each((function(){var e=0;$(this).find(".header-nav-folder-item:not(.header-child-accordion-next-level)").each((function(){e++,$(this).parent().attr("parent-count",+e)}))}))}))}function addHoverEvent(){$(".header-nav-list").each((function(){$(this).find(".header-nav-item--folder").each((function(){$(this).hover((function(e){e.preventDefault(),$(this).toggleClass("menu-item-active")}))}))}))}function numerateYesSecondLevelMenuItems(){$(".header-nav-item").each((function(){$(this).find(".header-child-accordion").each((function(){var e=0;$(this).find(".header-nav-folder-item").each((function(){e++,$(this).parent().attr("listing-count",+e)}))}))}))}function toggleHeaderNav(){$(window).scrollTop()>
=24?($(".header-nav-folder-content").fadeOut(200),$(".desktop-header-menu-accordion").removeClass("arrow-up")):$(".header-nav-folder-content").fadeIn(200)}$(document).ready((function(){prependMenuDeclarations()})),$(document).ready((function(){desktopMenuAccordion(),removeArrowOnHover()})),$(document).ready((function(){numerateNoSecondLevelMenuItems(),addHoverEvent(),numerateYesSecondLevelMenuItems()})),$(window).on("scroll",toggleHeaderNav); 
</script>


